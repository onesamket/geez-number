{
  "name": "@onesamket/geez-number",
  "version": "1.0.0",
  "description": "Convert between Arabic numerals and Geez (Ethiopic) numerals",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["geez", "ethiopic", "numeral", "number", "converter", "ethiopian", "eritrean"],
  "author": "onesamket",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.9",
    "typescript": "^5.0.0",
    "vitest": "^3.0.9"
  },
  "files": ["dist", "README.md", "LICENSE"],
  "vitest": {
    "testEnvironment": "node",
    "include": ["**/*.test.ts"],
    "alias": {
      "@": "./src"
    },
    "coverage": {
      "provider": "v8",
      "reporter": ["text", "html"]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onesamket/geez-number.git"
  },
  "bugs": {
    "url": "https://github.com/onesamket/geez-number/issues"
  },
  "homepage": "https://github.com/onesamket/geez-number#readme"
}
